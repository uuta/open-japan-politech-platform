{
  "name": "@ojpp/ingestion",
  "version": "0.1.0",
  "description": "OJPP データ取り込みパイプライン — 政治資金・国会会議録・マニフェスト",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "ingest:finance": "dotenv -e ../../.env -- tsx src/political-finance/ingest.ts",
    "ingest:parliament": "dotenv -e ../../.env -- tsx src/kokkai/ingest-sessions.ts && dotenv -e ../../.env -- tsx src/kokkai/ingest-bills.ts",
    "ingest:manifesto": "dotenv -e ../../.env -- tsx src/manifesto/seed-policies.ts",
    "ingest:elections": "dotenv -e ../../.env -- tsx src/elections/seed-elections.ts",
    "ingest:real-finance": "dotenv -e ../../.env -- tsx src/political-finance/seed-real-finance.ts",
    "ingest:councillors": "dotenv -e ../../.env -- tsx src/politicians/seed-councillors.ts",
    "ingest:representatives": "dotenv -e ../../.env -- tsx src/politicians/seed-representatives.ts",
    "ingest:real-bills": "dotenv -e ../../.env -- tsx src/kokkai/seed-real-bills.ts",
    "ingest:all": "pnpm ingest:finance && pnpm ingest:parliament && pnpm ingest:manifesto && pnpm ingest:elections && pnpm ingest:councillors && pnpm ingest:representatives",
    "test": "vitest run"
  },
  "dependencies": {
    "@ojpp/db": "workspace:*"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
